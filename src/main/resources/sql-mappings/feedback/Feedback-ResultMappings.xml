<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper    
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace= "Feedback-ResultMappings">
	<resultMap id="ResultForGetFeedbackForSurveyAndUser" type="Feedback" extends="ResultForGetFeedback">
		<result property="user.userId" column="USER_ID"/>
	</resultMap>

	<resultMap id="ResultForGetFeedbackForSurvey" type="Feedback" extends="ResultForGetFeedback">
		<result property="set.surveyId" column="SURVEY_ID"/>
	</resultMap>

	<resultMap id ="ResultForGetFeedback" type="Feedback">
		<result property="question.question" column="QUESTION"/>
		<result property="value" column="VALUE"/>
		<result property="option.risk" column="RISK"/>
		<result property="option.type" column="TYPE"/>
		<result property="option.displayName" column="DISPLAY_NAME"/>
	</resultMap>
</mapper>