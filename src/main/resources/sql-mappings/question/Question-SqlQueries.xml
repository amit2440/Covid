<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper    
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Question-SqlQueries">
	<sql id="SelectForGetQuestion">
		Q.QUESTION_ID,
		Q.QUESTION,
		Q.TYPE AS QUESTION_TYPE,
		O.OPTION_ID,
		O.DISPLAY_NAME,
		O.CHECKED,
		O.RISK
	</sql>
</mapper>