<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper    
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace= "UserInfo-ResultMappings">
	

	<resultMap id ="ResultForSearchUser" type="User">
		<result property="userId" column="user_id"/>
		<result property="firstName" column="first_name"/>
		<result property="lastName" column="last_name"/>
		<result property="role" column="role"/>
		<result property="mobile" column="mobile"/>
		<result property="workLocation" column="work_location"/>
		<result property="mgrID" column="mgr_id"/>
		<result property="managerName" column="mgrName"/>
	</resultMap>
</mapper>